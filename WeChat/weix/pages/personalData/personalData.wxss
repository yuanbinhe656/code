.detailWrap {
  padding: 15rpx;
  margin: 25rpx;
  border-radius: 10px;
  background-color: white;
}
.detailWrap .basicInfo {
  height: 70rpx;
  width: 100%;
  position: relative;
}
.detailWrap .basicInfo .icon {
  position: absolute;
  border-radius: 20rpx;
  left: 20rpx;
  top: 18rpx;
  width: 15rpx;
  height: 30rpx;
  background-color: #3cf53c;
}
.detailWrap .basicInfo .basic1 {
  position: absolute;
  top: 12rpx;
  left: 45rpx;
  font-size: 26rpx;
}
.detailWrap .basicInfo .edit {
    line-height : 45rpx;
  position: absolute;
  text-align: center;
  border-radius: 20px;
  top: 8rpx;
  left: 550rpx;
  width: 100rpx;
  height: 45rpx;
  font-weight: 600;
  font-size: 24rpx;
  background-color: #dbdbdb;
}
.detailWrap .nameWrap {
  height: 70rpx;
  width: 100%;
  display: flex;
  position: relative;
}
.detailWrap .nameWrap .name {
  position: absolute;
  font-weight: 600;
  left: 13rpx;
}
.detailWrap .nameWrap .name text {
  color: #2a9eeb;
}
.detailWrap .nameWrap .position {
  position: absolute;
  font-size: 24rpx;
